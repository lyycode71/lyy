#include<stdio.h>
int word(int a)
{
	if (a > 9)
	{
		word(a / 10);
	}
	printf("%d\n", a % 10);
}
int main()
{
	 word(19);
	return 0;
}
#include<stdio.h>
int bits(int a,int b)
{
	int num = 0;
	int t = a^b;
	while (t)
	{
		++num;
		t &= (t - 1);
	}
	return num;
}
int main()
{
	int c = bits(1999, 2299);
	printf("%d\n", c);
}

#include<stdio.h>
int main()
{
	int num = 0,i=0;
	int arr[32] = { 0 };
	scanf_s("%d", &num);
	if (num > 0)
	{
		for (i = 0; i < 32; i++)
		{
			arr[i] = num % 2;
			num = num / 2;
		}
		printf("偶数序列为：");
		for (i = 31; i >= 0; i-=2)
		{
			printf("%d", arr[i]);
		}
		printf("\n奇数序列为：");
		for (i = 30; i >= 0; i -= 2)
		{
			printf("%d",arr[i]);
		}
		printf("\n");
	}
	else
	{
		num = abs(num) - 1;
		for (i = 0; i < 32; i++)
		{
			arr[i] = !((num % 2) ^ 0);
			num = num / 2;
		}
		printf("偶数序列为：");
		for (i = 31; i >= 0; i -= 2)
		{
			printf("%d", arr[i]);
		}
		printf("\n奇数序列为：");
		for (i = 30; i >= 0; i -= 2)
		{
			printf("%d", arr[i]);
		}printf("\n");
	}return 0;
}
